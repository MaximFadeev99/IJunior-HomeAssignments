using System;
using System.Collections.Generic;

namespace CSTest2
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int[] originalNumbers1 = { 2, 3, 4, 6, 7, 8, 9 };
            int[] originalNumbers2 = { 1, 2, 4, 5, 6, 7, 9, 10 };
            List<int> mergedNumbers = new List<int>();

            mergedNumbers.AddRange(originalNumbers1);

            for (int i = 0; i < originalNumbers2.Length; i++) 
            {
                if (!mergedNumbers.Contains(originalNumbers2[i]))
                {
                    mergedNumbers.Add(originalNumbers2[i]);
                }
            }

            foreach (var  number in mergedNumbers) 
            {
                Console.WriteLine(number);
            }

            Console.ReadKey();
        }
    }
}
