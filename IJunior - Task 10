using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace CSTest1
{
    internal class Program
    {
        static void Main(string[] args)
        {        
            int startNumber = 5;
            int endNumber = 96;
            int step = 7;

            for (int i = startNumber; i <= endNumber; i += step)  //Выбрана конструкция For, поскольку у нас есть четкое понимание, когда итерации должны закончатся (когда число станет равно 96). 
            { 
                Console.WriteLine(i);
            }

            Console.ReadKey();
    }
}
