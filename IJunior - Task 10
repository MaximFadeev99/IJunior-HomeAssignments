using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace CSTest1
{
    internal class Program
    {
        static void Main(string[] args)
        {        
          int startNumber = 5;
          For (int i = startNumber, i = 96, i += 7)
          {
              Console.WriteLine(i)
          }
          Console.ReadKey()
        }
    }
}
