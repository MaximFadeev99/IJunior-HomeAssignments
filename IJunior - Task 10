using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace CSTest1
{
    internal class Program
    {
        static void Main(string[] args)
        {        
          int startNumber = 5;
          For (int i = startNumber, i = 96, i += 7) //Выбрана конструкция For, поскольку у нас есть четкое понимание, когда итерации должны закончатся (когда число станет равно 96). 
                                                     //Хотя с while тоже можно было бы прописать 
          {
              Console.WriteLine(i)
          }
          Console.ReadKey()
        }
    }
}
