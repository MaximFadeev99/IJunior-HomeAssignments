using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace CSTest1
{
    internal class Program
    {
        static void Main(string[] args)
        {        
          string surname = "Илья";
          string name = "Петров";
          surname = name;
          name = surname;
          console.writeline(surname);
          console.writeline(name);
          console.readkey();
        }
    }
}
